        "@material/select": "^5.1.0",
        "@material/snackbar": "^5.1.0",
        "@material/textfield": "^5.1.0",
        "@material/theme": "^5.1.0",
        "@rmwc/button": "^6.1.4",
        "@rmwc/card": "^6.1.4",
        "@rmwc/checkbox": "^6.1.4",
        "@rmwc/circular-progress": "^5.7.2",
        "@rmwc/drawer": "^6.1.4",
        "@rmwc/elevation": "^6.1.4",
        "@rmwc/formfield": "^6.1.4",
        "@rmwc/icon": "^6.1.4",
        "@rmwc/icon-button": "^5.7.2",
        "@rmwc/list": "^6.1.4",
        "@rmwc/menu": "^6.1.4",
        "@rmwc/select": "^6.1.4",
        "@rmwc/snackbar": "^6.1.4",
